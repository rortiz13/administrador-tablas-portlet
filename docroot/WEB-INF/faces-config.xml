<?xml version="1.0"?>

<faces-config
    version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
>
	
	<navigation-rule>
		<from-view-id>/config/*</from-view-id>
		<navigation-case>
			<from-outcome>gestionEspecialidades</from-outcome>
			<to-view-id>/config/especialidad/listado.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>gestionEntidades</from-outcome>
			<to-view-id>/config/entidad/listado.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>gestionCiudades</from-outcome>
			<to-view-id>/config/ciudad/listado.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>gestionUsuarios</from-outcome>
			<to-view-id>/config/usuario/agregar.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/config/especialidad/*</from-view-id>
		<navigation-case>
			<from-outcome>listado</from-outcome>
			<to-view-id>/config/especialidad/listado.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>agregar</from-outcome>
			<to-view-id>/config/especialidad/agregar.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>actualizar</from-outcome>
			<to-view-id>/config/especialidad/actualizar.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>eliminar</from-outcome>
			<to-view-id>/config/especialidad/eliminar.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>transaccionExitosa</from-outcome>
			<to-view-id>/config/especialidad/listado.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/config/entidad/*</from-view-id>
		<navigation-case>
			<from-outcome>listado</from-outcome>
			<to-view-id>/config/entidad/listado.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>agregar</from-outcome>
			<to-view-id>/config/entidad/agregar.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>actualizar</from-outcome>
			<to-view-id>/config/entidad/actualizar.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>eliminar</from-outcome>
			<to-view-id>/config/entidad/eliminar.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>transaccionExitosa</from-outcome>
			<to-view-id>/config/entidad/listado.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/config/ciudad/*</from-view-id>
		<navigation-case>
			<from-outcome>listado</from-outcome>
			<to-view-id>/config/ciudad/listado.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>agregar</from-outcome>
			<to-view-id>/config/ciudad/agregar.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>actualizar</from-outcome>
			<to-view-id>/config/ciudad/actualizar.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>eliminar</from-outcome>
			<to-view-id>/config/ciudad/eliminar.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>transaccionExitosa</from-outcome>
			<to-view-id>/config/ciudad/listado.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/config/usuario/*</from-view-id>
		<navigation-case>
			<from-outcome>agregar</from-outcome>
			<to-view-id>/config/usuario/agregar.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>eliminar</from-outcome>
			<to-view-id>/config/usuario/eliminar.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>transaccionExitosa</from-outcome>
			<to-view-id>/config/usuario/agregar.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<lifecycle>
		<phase-listener>com.liferay.faces.util.lifecycle.ViewScopePhaseListener</phase-listener>
	</lifecycle>
	
</faces-config>